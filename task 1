#include <iostream>
using namespace std;

int main() {
    int a, b, c;

    cout << "Enter three numbers: ";
    cin >> a >> b >> c;

    if (a <= b && a <= c) {
        if (b <= c)
            cout << "Ascending order: " << a << " " << b << " " << c;
        else
            cout << "Ascending order: " << a << " " << c << " " << b;
    }
    else if (b <= a && b <= c) {
        if (a <= c)
            cout << "Ascending order: " << b << " " << a << " " << c;
        else
            cout << "Ascending order: " << b << " " << c << " " << a;
    }
    else {
        if (a <= b)
            cout << "Ascending order: " << c << " " << a << " " << b;
        else
            cout << "Ascending order: " << c << " " << b << " " << a;
    }

    return 0;
}


